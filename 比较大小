#include <stdio.h>
int main (void)
{
	int a,b,c;
	
	printf ("putin a,b,c:");
	scanf ("%d%d%d",&a,&b,&c);
	if (a>b&&b>c)
		printf("%d,%d,%d",c,b,a);
	else if (a>b&&b<c)
		if(a>c)
			printf("%d,%d,%d",b,c,a);
		else printf("%d,%d,%d",b,a,c);
	else if (a<b&&b<c)
		printf("%d,%d,%d",a,b,c);
	else if (a<b&&b>c)
		if (a>c)
			printf("%d,%d,%d",c,a,b);
		else printf("%d,%d,%d",a,c,b);
		return 0;

	
}
